<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/2.5/template-install.dtd">
<extension version="3.0" type="template" client="site">
	<name>Blank Template</name>
	<version>1.0</version>
	<creationDate>21 March 2013</creationDate>
	<author>Fabio Rocha</author>
	<authorEmail>contato@solutibrasil.com.br</authorEmail>
	<authorUrl>http://solutibrasil.com.br</authorUrl>
	<copyright>Copyright (C) 2005 - 2013 Open Source Matters, Inc. All rights reserved.</copyright>
	<description>Blank Template for Joomla 3.*</description>

	<files>
		<folder>css</folder>
		<folder>images</folder>
		<folder>js</folder>
		<filename>index.html</filename>
		<filename>index.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<filename>favicon.ico</filename>
	</files>

	<positions>
		<position>breadcrumbs</position>
		<position>content</position>
		<position>debug</position>
		<position>footer</position>
		<position>left</position>
		<position>menu</position>
		<position>right</position>
		<position>search</position>
	</positions>

	<config>
	</config>

</extension>